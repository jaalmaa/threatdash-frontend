generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

type SessiondataCredentials {
  password String
  username String
}

model sessiondata {
  id          String                 @id @default(auto()) @map("_id") @db.ObjectId
  commands    String[]
  credentials SessiondataCredentials
  endTime     String
  sensor      String
  session     String
  shasum      String[]
  src_ip      String
  startTime   String
  url         String[]
}
